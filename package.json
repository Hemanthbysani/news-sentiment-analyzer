{
  "name": "news-sentiment-analyzer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "docker:build": "docker build -t news-sentiment-analyzer .",
    "docker:run": "docker run -p 3000:3000 news-sentiment-analyzer",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "setup": "./setup.sh",
    "seed": "curl -X POST http://localhost:3000/api/seed",
    "scrape": "curl -X POST http://localhost:3000/api/scraper",
    "scheduler:start": "curl -X POST http://localhost:3000/api/scheduler -H 'Content-Type: application/json' -d '{\"action\":\"start\"}'",
    "scheduler:stop": "curl -X POST http://localhost:3000/api/scheduler -H 'Content-Type: application/json' -d '{\"action\":\"stop\"}'",
    "scheduler:status": "curl http://localhost:3000/api/scheduler"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
    "cron": "^4.3.3",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "feedparser": "^2.2.10",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "mongoose": "^8.18.0",
    "newsapi": "^2.4.1",
    "next": "15.5.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "openai": "^5.19.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.2",
    "rss-parser": "^3.13.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/feedparser": "^2.2.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/xml2js": "^0.4.14",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
